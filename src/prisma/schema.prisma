generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Untagged {
  id        Int      @id @default(sequence())
  createdAt DateTime @default(now())
  url       String
}

model Tagged {
  id        Int      @id @default(sequence())
  idnum     Int
  createdAt DateTime @default(now())
  url       String
  tag       String
}
